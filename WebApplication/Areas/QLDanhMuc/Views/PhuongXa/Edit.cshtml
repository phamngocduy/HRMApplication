@model HRM.Databases.Models.dmPhuongXa


@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>dmPhuongXa</legend>

        @Html.HiddenFor(model => model.id)

        <div class="editor-label">
            @Html.LabelFor(model => model.maPhuongXa)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.maPhuongXa)
            @Html.ValidationMessageFor(model => model.maPhuongXa)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.tenPhuongXa)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.tenPhuongXa)
            @Html.ValidationMessageFor(model => model.tenPhuongXa)
        </div>
         <div class="editor-label">
           Tên Quận Huyện
        </div>
        <div class="editor-field">
            @Html.DropDownList("idQuanHuyen", string.Empty)
            @Html.ValidationMessageFor(model => model.idQuanHuyen)
        </div>
        <p>
            <input class="dialog" type="submit" value="Save" />
        </p>
    </fieldset>
}

@*
@model IEnumerable<HRM.QLBHXH.Models.nvbhNhanVienBHXH>
@{
    ViewBag.Title = "BHXH";
}
<input type="button" value="Tạo mới NV BHXH" onclick="location.href='@Url.Action("Create")'" />
<input type="button" value="Danh mục" id="flip"" />
<input type="button" value="ĐC tăng giảm" onclick="location.href='@Url.Action("index", "DanhSachDieuChinhTangGiam")'" />
<div id="panel">
    @Html.ActionLink("Mức lương tối thiểu chung", "DanhMuc")
    @Html.ActionLink("Mức lương tối thiểu vùng", "DanhMuc")
    @Html.ActionLink("Tỷ lệ đóng bảo hiểm", "DanhMuc")
</div>
<div id="ChiTiet">
</div>


<table id="NVBHXH">
    <thead>
  


        <tr>
            <th style="text-align:center" nowrap>
                Mã NV
            </th>
            <th style="text-align:center" nowrap>
                Họ và tên
            </th>
            <th style="text-align:center" nowrap>
                Đơn vị
            </th>
            <th style="text-align:center" nowrap>
                Số sổ BHXH
            </th>
            <th style="text-align:center" nowrap>
                Số phiếu KCB
            </th>
            <th style="text-align:center" nowrap>
                Ngày sinh
            </th>
            <th style="text-align:center" nowrap>
                Nữ
            </th>
            <th style="text-align:center" nowrap>
                CMND
            </th>
            <th style="text-align:center" nowrap>
                Chức danh nghề nghiệp
            </th>
            <th style="text-align:center" nowrap>
                Địa chỉ thường trú
            </th>
            <th style="text-align:center" nowrap>
                Nơi đăng kí KCB
            </th>
            <th style="text-align:center" nowrap>
                HS lương
            </th>
            <th style="text-align:center" nowrap>
                ML đóng BHXH, BHYT
            </th>
            <th style="text-align:center" nowrap>
                ML đóng BHTN
            </th>
            <th style="text-align:center" nowrap>
                Tổng tiền nộp BHXH, BHYT
            </th>
            <th style="text-align:center" nowrap>
                Tổng tiền nộp BHTN
            </th>
            <th style="text-align:center" nowrap>
                TG tham gia BHXH
            </th>
            <th style="text-align:center" nowrap>
                TG tham gia BHTN
            </th>
            <th style="text-align:center" nowrap>
                TG băt đầu nghỉ TS
            </th>
            <th style="text-align:center" nowrap>
                TG kết thúc nghỉ TS
            </th>
            <th style="text-align:center" nowrap>
                Ghi chú
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.MANV)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.HoVaTen)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.DonVi)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.SoBHXH)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.SoKCB)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.NgaySinh)
                </td>
                <td style="text-align:left" nowrap>
                    @if (item.Nu == true)
                    {
                        <span>
                            @Html.CheckBox("check1", true)
                        </span>
                    }
                    else
                    {
                        <span>
                            @Html.CheckBox("check1", false)
                        </span>
                    }
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.CMND)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.ChucDanh)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.DiaChiTT)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.dmNoiKCB.TenBenhVien)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.HeSoLuong)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.MucLuongDongBHXHVaBHYT)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.MucLuongDongBHTN)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.TongSoTienNopBHXHVaBHYT)
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.TongSoTienNopBHTN)
                </td>
                <td style="text-align:left" nowrap>
                    @item.ThoiGianThamGiaBHXH.GetValueOrDefault().ToShortDateString()
                </td>
                <td style="text-align:left" nowrap>
                    @item.ThoiGianThamGiaBHTN.GetValueOrDefault().ToShortDateString()
                </td>
                <td style="text-align:left" nowrap>
                    @item.ThoiGianBatDauNghiTS.GetValueOrDefault().ToShortDateString()
                </td>
                <td style="text-align:left" nowrap>
                    @item.ThoiGianKetThucNghiTS.GetValueOrDefault().ToShortDateString()
                </td>
                <td style="text-align:left" nowrap>
                    @Html.DisplayFor(modelItem => item.GhiChu)
                </td>
                <td>
                    <a href="NhanVienBHXH/Delete?id=@item.id" title="Xóa"><span class="ui-icon ui-icon-trash"
                        style="display: inline-block" /></a>
                </td>
            </tr>
        }
    </tbody>
</table>
<div id="tabs"></div>




<script src="/Scripts/datatables/FixedColumns.js"></script>
<script src="/Content/datatables/DataTableFixedHeader.js"></script>
<script>


    $.get("../NhanVienBHXH/Create").success(function (data) {
        $('#tabs').html(data);
    });


    $(document).ready(function () {
        var oTable = $('#NVBHXH').dataTable({
            "bJQueryUI": true,
            "sScrollY": "100%",
            "sScrollX": "100%"


        });
        //        new FixedHeader(oTable, { "bottom": true });

        new $.fn.dataTable.FixedColumns(oTable, {
            "iLeftColumns": 2,
            "iRightColumns": 1
        });


    });




    $("#flip").click(function () {
        $("#panel").slideToggle("fast");
    });







</script>
<style type="text/css">
    #panel
    {
        text-align: left;
        background-color: #e5eecc;
        border: solid 1px #c3c3c3;
    }
    #panel
    {
        display: none;
    }
</style>

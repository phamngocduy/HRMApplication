            <span class="tile-title accent">carousel slide 2</span>
        </div>
        <div>
            <img class="full" src="/images/sample/3tw.gif" alt="3" />
            <span class="tile-title">carousel slide 3</span>
        </div>
        <div>
            <img class="full" src="/images/sample/4t.gif" alt="4" />
            <span class="tile-title accent">carousel slide 4</span>
        </div>
    </div>
    <!-- Mango Flip List that triggers every 3 seconds -->
    <div class="list-tile mango">
        <span class="tile-title">flip list</span>
        <ul class="flip-list fourTiles" data-mode="flip-list" data-delay="2000">
            <li>
            
                <div><img src="http://localhost:59714/Scripts/MetroJs/images/all-new-htc-one-m8-wallpaper-6.jpg" alt="1" /></div>
                <div>keroro</div>
            </li>
            <li>
                <div><img src="http://localhost:59714/Scripts/MetroJs/images/all-new-htc-one-m8-wallpaper-6.jpg" alt="2" /></div>
                <div>nghia</div>
            </li>
            <li>
                <div><img src="http://localhost:59714/Scripts/MetroJs/images/all-new-htc-one-m8-wallpaper-6.jpg" alt="3" /></div>
                <div>le</div>
            </li>
            <li data-direction="horizontal">
                <div><img src="http://localhost:59714/Scripts/MetroJs/images/all-new-htc-one-m8-wallpaper-6.jpg" alt="4" /></div>
                <div>sims team</div>
            </li>
        </ul>
    </div>
    <!-- Green Static Tile -->
    <div class="live-tile accent green exclude">
        <span class="tile-title">static tile</span>
        <p>Static tiles can take advantage of theming too!</p>
    </div>
</div>





<!-- Activate live tiles -->
<script type="text/javascript">
    // apply regular slide universally unless .exclude class is applied 
    // NOTE: The default options for each liveTile are being pulled from the 'data-' attributes
    $(".live-tile, .flip-list").not(".exclude").liveTile().click(function () {
        

         window.location = "@Url.Action("Index2", "NhanVienBHXH")" ;
    }); ;
 

</script>
